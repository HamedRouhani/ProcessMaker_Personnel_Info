        <dyn_content><![CDATA[{"name":"درخواست ثبت / اصلاح اطلاعات فردي","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"63961791962df6f6d9817e0033638766","name":"درخواست ثبت / اصلاح اطلاعات فردي","description":"","mode":"edit","script":{"type":"js","code":"// نمايش صفحه در لود اوليه\n\n$(\"#63961791962df6f6d9817e0033638766\").css({\"backgroundColor\":\"#ccccff\"});\n$(\"#63961791962df6f6d9817e0033638766\").css({\"border\":\"3px double #336699\"});\n\n\n$(\"#title0000000001\").css({\"font-family\":\"b Titr\",\"color\":\"#ffffff\",\"background-color\":\"#336699\",\"border-radius\":\"5px\"});\n$(\"#title0000000001\").css({\"text-align\":\"center\"});\n\n$(\"#subtitle0000000001\").css({\"font-family\":\"b Titr\",\"color\":\"#ffffff\",\"background-color\":\"#66CC33\",\"border-radius\":\"5px\"});\n$(\"#subtitle0000000001\").css({\"text-align\":\"center\"});\n\n$(\"#Description\").css({\"color\":\"white\",\"background-color\":\"green\",\"border-radius\":\"5px\"});\n\n$(\"#Ext_Jobs_Finder\").css({\"color\":\"white\",\"background-color\":\"#663399\",\"border-radius\":\"5px\"});\n\n$(\"#Int_Jobs_Finder\").css({\"color\":\"white\",\"background-color\":\"#993366\",\"border-radius\":\"5px\"});\n\n$(\"#submit0000000001\").css({\"font-family\":\"b titr\"});\n\n$(\"#Ext_Int_ID\").hide();\n$(\"#Job_Counter\").hide();\n$(\"#submit0000000001\").hide();\n$(\"#ID_Detector_Search\").hide();\n$(\"#ID_Detector\").hide();\n$(\"#First_Name\").hide();\n$(\"#Last_Name\").hide();\n$(\"#Personnel_Code\").hide();\n$(\"#National_Code\").hide();\n$(\"#Email\").hide();\n$(\"#Cell_Phone\").hide();\n$(\"#Ext_Int_ID_Show\").hide();\n$(\"#System_Personal_Code\").hide();\n$(\"#Ext_Jobs_Finder\").hide();\n$(\"#Int_Jobs_Finder\").hide();\n\n//پر كردن شرح و توضيحات  --- \n\nif ($(\"#Description\").getNumberRows() == 1 && $(\"#Description\").getValue (1,1) ==\"\") {\n\t$('#Description').getControl($(\"#Description\").getNumberRows(),'2').attr('disabled',false);\n  \t$(\"#Description\").setValue($(\"#txt_name\").getValue(),$(\"#Description\").getNumberRows(),'1');\n  }else{\n\t$(\"#Description\").addRow();\n\t$('#Description').getControl($(\"#Description\").getNumberRows(),'2').attr('disabled',false); \n    $(\"#Description\").setValue($(\"#txt_name\").getValue(),$(\"#Description\").getNumberRows(),'1');\n  };\n\n// نمايش سمت براي فرد خارج /داخل سازمان بصورت اتوماتيك\n\n$(\"#Ext_Jobs_Finder\").deleteRow();\n$(\"#Int_Jobs_Finder\").deleteRow();\n\n$(\"#ID_Detector\").setOnchange(function(EI,SPC,JCR,IDD) {\n  \n  EI = $(\"#Ext_Int_ID\").getValue();\n  IDD = $(\"#ID_Detector\").getValue();\n  SPC = $(\"#System_Personal_Code\").getValue();\n  JCR = $(\"#ORG_Job_Code_Rank_Finder\").getValue();\n  \n  switch (true) {\n      \n    case (EI == 'X' && IDD != \"\"):\n      \n      $(\"#Ext_Jobs_Finder\").show();\n      $(\"#Int_Jobs_Finder\").hide();\n      $(\"#submit0000000001\").show();\n      $(\"#Ext_Int_ID_Show\").setValue(\"خارجي\");\n\n  \t\tvar rows = $(\"#Ext_Jobs_Finder\").getNumberRows();\n    \t\tfor (var i=1; i <= rows; i++) {\n        \t\t$(\"#Ext_Jobs_Finder\").deleteRow();\n    \t\t}\n  \n  \t\tfor (var i= 1 ; i <= parseInt($(\"#Job_Counter\").getValue()); i++) {\n\n    \t\t$(\"#Ext_Jobs_Finder\").addRow();\n    \t\t$(\"#Ext_Jobs_Finder\").setValue(EI,i,4);\n    \t\t$(\"#Ext_Jobs_Finder\").setValue(SPC,i,5);\n    \t\t$(\"#Ext_Jobs_Finder\").setValue(i,i,6);\n    \n    \t\tif ($(\"#Ext_Jobs_Finder\").getValue(i,7) == 'M') {\n      \n      \t\t\t$(\"#Ext_Jobs_Finder\").setValue(\"اصلي\",i,1);\n\n    \t\t} else if ($(\"#Ext_Jobs_Finder\").getValue(i,7) == 'D') {\n      \n      \t\t\t$(\"#Ext_Jobs_Finder\").setValue(\"فرعي\",i,1);\n      \n    \t\t}\n    \n  \t\t}\n      \n\n    case (EI == 'N' && IDD !=\"\"):\n\n      $(\"#Ext_Jobs_Finder\").hide();\n      $(\"#Int_Jobs_Finder\").show();\n      $(\"#submit0000000001\").show();\n      $(\"#Ext_Int_ID_Show\").setValue(\"داخلي\");\n      \n  \t\tvar rows = $(\"#Int_Jobs_Finder\").getNumberRows();\n    \t\tfor (var i=1; i <= rows; i++) {\n        \t\t$(\"#Int_Jobs_Finder\").deleteRow();\n    \t\t}\n  \n  \t\tfor (var i= 1 ; i <= parseInt($(\"#Job_Counter\").getValue()); i++) {\n\n    \t\t$(\"#Int_Jobs_Finder\").addRow();\n    \t\t$(\"#Int_Jobs_Finder\").setValue(EI,i,4);\n    \t\t$(\"#Int_Jobs_Finder\").setValue(SPC,i,5);\n    \t\t$(\"#Int_Jobs_Finder\").setValue(i,i,6);\n    \n    \t\tif ($(\"#Int_Jobs_Finder\").getValue(i,7) == 'M') {\n      \n      \t\t\t$(\"#Int_Jobs_Finder\").setValue(\"اصلي\",i,1);\n\n    \t\t} else if ($(\"#Int_Jobs_Finder\").getValue(i,7) == 'D') {\n      \n      \t\t\t$(\"#Int_Jobs_Finder\").setValue(\"فرعي\",i,1);\n      \n    \t\t}\n          \n  \t\t} \n\n  }\n\n});\n\n// كنترل فيلدهاي فرم\n\n$(\"#Select_Reg_Type\").setOnchange(function(RST) {\n  \n  RST = $(\"#Select_Reg_Type\").getValue();\n  \n  switch (true) {\n      \n    case ( RST == '') : \n      $(\"#submit0000000001\").hide();\n      $(\"#ID_Detector_Search\").hide();\n      $(\"#ID_Detector\").hide();\n      $(\"#First_Name\").hide();\n      $(\"#Last_Name\").hide();\n      $(\"#Personnel_Code\").hide();\n      $(\"#National_Code\").hide();\n      $(\"#Email\").hide();\n      $(\"#Cell_Phone\").hide();\n      $(\"#Ext_Int_ID_Show\").hide();\n      $(\"#System_Personal_Code\").hide();\n      $(\"#Ext_Jobs_Finder\").hide();\n      $(\"#Int_Jobs_Finder\").hide();\n      $(\"#ID_Detector\").setValue(\"\");\n      break;      \n      \n    case ( RST == '1') : \n      $(\"#submit0000000001\").show();\n      $(\"#ID_Detector_Search\").hide();\n      $(\"#ID_Detector\").hide();\n      $(\"#First_Name\").hide();\n      $(\"#Last_Name\").hide();\n      $(\"#Personnel_Code\").hide();\n      $(\"#National_Code\").hide();\n      $(\"#Email\").hide();\n      $(\"#Cell_Phone\").hide();\n      $(\"#Ext_Int_ID_Show\").hide();\n      $(\"#System_Personal_Code\").hide();\n      $(\"#Ext_Jobs_Finder\").hide();\n      $(\"#Int_Jobs_Finder\").hide();\n      $(\"#ID_Detector\").setValue(\"\");\n      break;\n      \n    case (RST == '2' || RST == '3') : \n      $(\"#submit0000000001\").hide();\n      $(\"#ID_Detector_Search\").show();\n      $(\"#ID_Detector\").show();\n      $(\"#First_Name\").show();\n      $(\"#Last_Name\").show();\n      $(\"#Personnel_Code\").show();\n      $(\"#National_Code\").show();\n      $(\"#Email\").show();\n      $(\"#Cell_Phone\").show();\n      $(\"#Ext_Int_ID_Show\").show();\n      $(\"#System_Personal_Code\").show();\n      break;\n      \n  }\n  \n});\n  \n        \n$(\"#63961791962df6f6d9817e0033638766\").setOnchange(function(IDD,RST) {\n  \n  IDD = $(\"#ID_Detector\").getValue();\n  RST = $(\"#Select_Reg_Type\").getValue();\n  \n  if (IDD == \"\" && RST !=\"1\"){\n    \n    $(\"#submit0000000001\").hide();\n    $(\"#Ext_Jobs_Finder\").hide();\n    $(\"#Int_Jobs_Finder\").hide();\n    \n  }\n  \n});\n\n"},"language":"en","externalLibs":"","printable":false,"items":[[{"type":"title","id":"title0000000001","label":"درخواست ثبت / اصلاح اطلاعات فردي","colSpan":12}],[{"type":"grid","variable":"Description","var_uid":"16055297862df720865d7d5021609619","dataType":"grid","protectedValue":false,"id":"Description","name":"Description","label":"درخواست / نظرات","hint":"","required":false,"columns":[{"type":"text","variable":"","var_uid":"","dataType":"","protectedValue":false,"id":"txt_name1","name":"txt_name1","label":"نام و نام خانوادگي","defaultValue":"","placeholder":"","hint":"","required":false,"textTransform":"none","validate":"","validateMessage":"","maxLength":1000,"formula":"","mode":"parent","operation":"","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","columnWidth":"30%","width":100,"title":"نام و نام خانوادگي"},{"type":"textarea","variable":"","var_uid":"","dataType":"","protectedValue":false,"id":"Desc_Details","name":"Desc_Details","label":"شرح","defaultValue":"","placeholder":"","hint":"","required":false,"validate":"","validateMessage":"","mode":"parent","dbConnection":